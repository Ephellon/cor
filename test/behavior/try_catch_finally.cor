
func init() {
    describe('Try Catch Finally, Should Work for', func() {

        it('Only Try', func() {
            boom = func() {
                try { throw 'e' }
            }
            expect(boom).not.toThrow()
        })

        it('Try and Catch', func() {
            boom = func() {
                try {
                    throw 'e'
                } catch e {
                    return 5
                }
            }
            expect(boom).not.toThrow()
            expect(boom()).toBe(5)
        })

        it('Try, Catch and Finally', func() {
            boom = func() {
                try {
                    // no throw
                } catch e {
                    return 5
                } finally {
                    return 6
                }
            }
            expect(boom).not.toThrow()
            expect(boom()).toBe(6)
        })

    })
}
