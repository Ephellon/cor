
func init() {
    describe('Break Statement', func() {
        it('Should break For Statements', func() {
            a = 1
            for {
                break
                a = 2
            }
            expect(a).toBe(1)

            b = 1
            for {
                b = 2
                break
            }
            expect(b).toBe(2)
        })
    })
}
