
func init() {
    describe('Lambda Expression', func() {
        
        it('Should work with literals', func() {
            f = func() 1
            expect(f()).toBe(1)
        })
        
        it('Should work with expression', func() {
            f = func(x) x + 5
            expect(f(5)).toBe(10)
        })


        it('Should work with expression', func() {
            f = func () [
                1, 2,
            ]
                
            expect(f()).toEqual([1, 2])
        })
        
        
    })
}
